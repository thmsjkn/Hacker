<nav class="pagination">
  <% if (page.prev){ %>
  <a href="<%- config.root %><%- page.prev_link %>index.html" class="pagination-prev"><%= __('prev') %></a>
  <% } %>
  <% if (page.next){ %>
  <a href="<%- config.root %><%- page.next_link %>index.html" class="pagination-next"><%= __('next') %></a>
  <% } %>
</nav>